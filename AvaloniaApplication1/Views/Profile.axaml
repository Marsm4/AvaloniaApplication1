<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="300"
        x:Class="AvaloniaApplication1.Profile"
        Title="Редактирование пользователя">
	<Grid RowDefinitions="Auto, Auto, Auto, Auto, Auto, Auto, Auto" Margin="10">
		<TextBlock Grid.Row="0">Имя</TextBlock>
		<TextBox Grid.Row="1" x:Name="tbName" Margin="0,0,0,10"/>
		<TextBlock Grid.Row="2">Email</TextBlock>
		<TextBox Grid.Row="3" x:Name="tbEmail" Margin="0,0,0,10"/>
		<TextBlock Grid.Row="4">Password</TextBlock>
		<TextBox Grid.Row="5" x:Name="tbPassword" Margin="0,0,0,10"/>
		<Button Content="Сохранить" Grid.Row="6" Click="SaveButton_Click"/>
	</Grid>
</Window>